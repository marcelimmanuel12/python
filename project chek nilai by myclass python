from MyModule import MyClass

print("=== Program ngchek nilai ===")

grade = input("Masukan grade Anda (A / B /C / D /E) :")
grade = grade.upper()

# buat variabel baru terhadap MyClass
# istilah teknis : create new instance of MyClass
# menerapkan konsep / paradigma OOP
mc = MyClass()
nilai = mc.chekNilai(grade)
print(f"Grade = {grade}\nNilai = {nilai}")
